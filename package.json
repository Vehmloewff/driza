{
	"name": "versatilejs",
	"version": "0.2.0-next.6",
	"description": "Still in development... Check back soon!",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"browser": "dist/index.browser.js",
	"typings": "typings/runtime/index.d.ts",
	"scripts": {
		"build": "cross-env NODE_ENV=production rollup -c && tsc",
		"test": "cross-env NODE_ENV=development cross-env rollup -c",
		"test:runtime": "cross-env VERSATILE_FILTER='src/runtime' npm run test --",
		"test:workflow": "cross-env VERSATILE_FILTER='src/workflow-manager' npm run test --",
		"test:compiler": "cross-env VERSATILE_FILTER='src/compiler' npm run test --",
		"lint": "prettier --write \"./**\"",
		"lint:test": "prettier --check \"./**\"",
		"preversion": "npm run test && npm run lint:test && npm run build",
		"prepublishOnly": "npm run build",
		"postversion": "git push && git push --tags && rm -rf build/temp && npm publish"
	},
	"keywords": [
		"todo",
		"todo",
		"todo"
	],
	"author": {
		"name": "Vehmloewff",
		"url": "https://github.com/Vehmloewff"
	},
	"files": [
		"compiler",
		"internal",
		"store",
		"workflow",
		"style",
		"dist",
		"typings",
		"easing"
	],
	"license": "MIT",
	"devDependencies": {
		"cross-env": "^6.0.3",
		"prettier": "^1.19.1",
		"rollup-plugin-command": "^1.0.7",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-glob-files": "^1.0.2",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-typescript": "^1.0.1",
		"tslib": "^1.10.0",
		"typescript": "^3.7.3",
		"zip-tap": "^1.0.4",
		"zip-tap-reporter": "^1.0.4"
	},
	"homepage": "https://github.com/Vehmloewff/versatilejs",
	"repository": {
		"type": "git",
		"url": "https://github.com/Vehmloewff/versatilejs"
	},
	"dependencies": {
		"rollup": "^1.27.9"
	}
}
